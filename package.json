{
  "name": "@sysdotini/eslint-config",
  "version": "1.0.2",
  "description": "My strict and opinionated sharable ESLint config with built-in TypeScript and Prettier support.",
  "author": "sysdotini <espi@riseup.net> (https://espi.me)",
  "license": "MIT",
  "homepage": "https://github.com/sysdotini/eslint-config",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/sysdotini/eslint-config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysdotini/eslint-config.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "quality",
    "style",
    "lint",
    "linter",
    "eslint",
    "config",
    "eslint config",
    "code style",
    "strict",
    "prettier"
  ],
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "quoteProps": "consistent",
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "useTabs": false
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0"
  },
  "peerDependencies": {
    "eslint": "^7.26.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  }
}
